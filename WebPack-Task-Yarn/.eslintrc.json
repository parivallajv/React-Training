{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb","eslint:recommended","plugin:react/recommended","prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "react/jsx-filename-extension": ["error", { "extensions": ["js", ".jsx"] }]
    }
}